{
  "TOC": {
    "prefix": "toc",
    "body": ["# TOC", "- ${1}", "  - ${2}"],
    "description": "Table of Contents"
  },
  "header 1": {
    "prefix": "h1",
    "body": ["# ${0}"],
    "description": "Add header level 1"
  },
  "header 2": {
    "prefix": "h2",
    "body": ["## ${0}"],
    "description": "Add header level 2"
  },
  "header 3": {
    "prefix": "h3",
    "body": ["### ${0}"],
    "description": "Add header level 3"
  },
  "header 4": {
    "prefix": "h4",
    "body": ["#### ${0}"],
    "description": "Add header level 4"
  },
  "header 5": {
    "prefix": "h5",
    "body": ["##### ${0}"],
    "description": "Add header level 5"
  },
  "header 6": {
    "prefix": "h6",
    "body": ["###### ${0}"],
    "description": "Add header level 6"
  },
  "Links": {
    "prefix": ["l"],
    "body": ["[${1}](${2})${0}"],
    "description": "Add links"
  },
  "URLS": {
    "prefix": ["u"],
    "body": ["<${1}>${0}"],
    "description": "Add urls"
  },
  "Images": {
    "prefix": "img",
    "body": ["![${1}](${2})${0}"],
    "description": "Add images"
  },
  "Insert strikethrough": {
    "prefix": "strkt",
    "body": "~~${1}~~${0}",
    "description": "Insert strikethrough"
  },
  "Insert bold text": {
    "prefix": ["b"],
    "body": "**${1}**$0",
    "description": "Insert bold text"
  },
  "Insert italic text": {
    "prefix": ["i"],
    "body": "*${1}*$0",
    "description": "Insert italic text"
  },
  "Insert bold and italic text": {
    "prefix": ["bi"],
    "body": "***${1}***$0",
    "description": "Insert bold and italic text"
  },
  "Insert quoted text": {
    "prefix": "qt",
    "body": "> ${1}",
    "description": "Insert quoted text"
  },
  "Insert code": {
    "prefix": "code",
    "body": "`${1}` $0",
    "description": "Insert code"
  },
  "Insert code block": {
    "prefix": "codeblock",
    "body": ["```${1:language in lowercase}", "  $0", "```"],
    "description": "Insert fenced code block into markdown"
  },
  "Insert unordered list": {
    "prefix": "uls",
    "body": ["- ${1:first}", "- ${2:second}", "- ${3:third}", "$0"],
    "description": "Insert unordered list"
  },
  "Insert ordered list": {
    "prefix": "ols",
    "body": ["1. ${1:first}", "2. ${2:second}", "3. ${3:third}", "$0"],
    "description": "Insert ordered list"
  },
  "Insert horizontal rule": {
    "prefix": "hrule",
    "body": "----------\n",
    "description": "Insert horizontal rule"
  },
  "Insert task list": {
    "prefix": "task",
    "body": ["- [${1| ,x|}] ${2:text}", "${0}"],
    "description": "Insert task list"
  },
  "Insert task list 2": {
    "prefix": "task2",
    "body": ["- [${1| ,x|}] ${2:text}", "- [${3| ,x|}] ${4:text}", "${0}"],
    "description": "Insert task list with 2 tasks"
  },
  "Insert task list 3": {
    "prefix": "task3",
    "body": [
      "- [${1| ,x|}] ${2:text}",
      "- [${3| ,x|}] ${4:text}",
      "- [${5| ,x|}] ${6:text}",
      "${0}"
    ],
    "description": "Insert task list with 3 tasks"
  },
  "Insert task list 4": {
    "prefix": "task4",
    "body": [
      "- [${1| ,x|}] ${2:text}",
      "- [${3| ,x|}] ${4:text}",
      "- [${5| ,x|}] ${6:text}",
      "- [${7| ,x|}] ${8:text}",
      "${0}"
    ],
    "description": "Insert task list with 4 tasks"
  },
  "Insert task list 5": {
    "prefix": "task5",
    "body": [
      "- [${1| ,x|}] ${2:text}",
      "- [${3| ,x|}] ${4:text}",
      "- [${5| ,x|}] ${6:text}",
      "- [${7| ,x|}] ${8:text}",
      "- [${9| ,x|}] ${10:text}",
      "${0}"
    ],
    "description": "Insert task list with 5 tasks"
  },
  "Insert table": {
    "prefix": "tbl",
    "body": [
      "| ${1:Column1}  | ${2:Column2}   | ${3:Column3}   |",
      "|-------------- | -------------- | -------------- |",
      "| ${4:Item1}    | ${5:Item1}     | ${6:Item1}     |",
      "${0}"
    ],
    "description": "Insert table with 2 rows and 3 columns. First row is heading."
  },
  "Insert 2x1 table": {
    "prefix": "2x1tbl",
    "body": [
