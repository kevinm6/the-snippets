  "Markdown Head": {
    "prefix": "head",
    "body": ["# ${1:${TM_FILENAME}}\n", "---\n", "$0"],
    "description": "Markdown files head (info)"
  },
  "file-info_yaml": {
    "prefix": "infYaml",
    "body": [
      "---",
      "File          : ${TM_FILENAME}",
      "Author        : Kevin",
      "Date          : ${CURRENT_DATE}/${CURRENT_MONTH}/${CURRENT_YEAR}, ${CURRENT_HOUR}:${CURRENT_MINUTE}",
      "output        :",
      "\tpdf_document: default",
      "\thtml_notebook: default",
      "---\n",
      "# ${1:Title}\n",
      "---\n",
      "$0"
    ],
    "description": "file info in yaml for md files"
  },
  "Right Arrow": {
    "prefix": ["rarr", "->"],
    "body": ["&rarr;"],
    "description": "Insert Right Arrow"
  },
  "Left Arrow": {
    "prefix": "larr",
