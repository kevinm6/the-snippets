{
  "autocmd": {
    "body": [
      "vim.api.nvim_create_autocmd(\"${1:Event}\", {",
      "\tpattern = \"${2:pattern}\",",
      "\tcallback = function()",
      "\t\t$0",
      "\tend,",
      "})"
    ],
    "prefix": "autocmd",
    "description": "Create a Nvim autocmd"
  },
  "file exists": {
    "body": "local fileExists = vim.uv.fs_stat(\"${1:filepath}\") ~= nil",
    "prefix": "file exists"
  }
}
