	"Date&Time": {
    "prefix": "datetime",
    "body": [
      "${CURRENT_DATE}/${CURRENT_MONTH}/${CURRENT_YEAR} - ${CURRENT_HOUR}:${CURRENT_MINUTE}"
    ],
